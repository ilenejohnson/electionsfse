{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "customTemplates": [
    {
      "name": "blank",
      "postTypes": ["page", "post"],
      "title": "Blank"
    },

    {
      "name": "404",
      "postTypes": ["page"],
      "title": "404"
    }
  ],
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "header-small-text",
      "title": "Header Small Text",
      "area": "header"
    },
    {
      "name": "header-minimal-right-nav",
      "title": "Header minimal, right nav",
      "area": "header"
    },
    {
      "name": "header-with-image",
      "title": "Header with image",
      "area": "header"
    },
    {
      "name": "header-two-tier",
      "title": "Two tiered header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "settings": {
    "typography": {
      "dropCap": true,
      "lineHeight": true,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen-Sans,Ubuntu,Cantarell,Helvetica Neue,sans-serif",
          "slug": "system-fonts",
          "name": "System fonts"
        },
        {
          "fontFamily": "Helvetica Neue, Helvetica, Arial, sans-serif",
          "slug": "helvetica-arial",
          "name": "Helvetica or Arial"
        },
        {
          "fontFamily": "\"Poppins\"",
          "name": "Poppins",
          "slug": "poppins",
          "fontFace": [
            {
              "fontFamily": "Poppins",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Poppins-Regular.woff2"]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Poppins-Italic.woff2"]
            }
          ]
        },
        {
          "fontFamily": "\"Montserrat\"",
          "name": "Montserrat",
          "slug": "montserrat",
          "fontFace": [
            {
              "fontFamily": "Montserrat",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Montserrat-VariableFont_wght.woff2"]
            },
            {
              "fontFamily": "Montserrat",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/Montserrat-Italic-VariableFont_wght.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "\"Raleway\"",
          "name": "Raleway",
          "slug": "raleway",
          "fontFace": [
            {
              "fontFamily": "Raleway",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Raleway-VariableFont_wght.woff2"]
            },
            {
              "fontFamily": "Raleway",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/Raleway-Italic-VariableFont_wght.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "\"LibreBaskerville\"",
          "name": "LibreBaskerville",
          "slug": "libre-baskerville",
          "fontFace": [
            {
              "fontFamily": "LibreBaskerville",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/LibreBaskerville-Regular.woff2"]
            },
            {
              "fontFamily": "LibreBaskerville",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/LibreBaskerville-Italic.woff2"]
            }
          ]
        },
        {
          "fontFamily": "\"Lato\"",
          "name": "Lato",
          "slug": "lato",
          "fontFace": [
            {
              "fontFamily": "Lato",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Lato-Regular.woff2"]
            },
            {
              "fontFamily": "Lato",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Lato-Italic.woff2"]
            }
          ]
        },

        {
          "fontFamily": "\"Oswald\"",
          "name": "Oswald",
          "slug": "oswald",
          "fontFace": [
            {
              "fontFamily": "Oswald",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Oswald-VariableFont_wght.woff2"]
            },
            {
              "fontFamily": "Oswald",
              "fontWeight": "300 400 500 600 700 900",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Oswald-VariableFont_wght.woff2"]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "fluid": {
            "min": "0.875rem",
            "max": "1rem"
          },
          "size": "1rem",
          "slug": "small"
        },
        {
          "fluid": {
            "min": "1rem",
            "max": "1.25rem"
          },
          "size": "1.25rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "min": "1.25rem",
            "max": "1.575rem"
          },
          "size": "1.5rem",
          "slug": "large"
        },
        {
          "fluid": {
            "min": "2rem",
            "max": "2.85rem"
          },
          "size": "2.125rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "min": "2.8rem",
            "max": "3.85rem"
          },
          "size": "3.00em",
          "slug": "xx-large"
        },
        {
          "fluid": {
            "min": "3.00rem",
            "max": "3.85rem"
          },
          "size": "3.5rem",
          "slug": "xxx-large"
        },
        {
          "fluid": {
            "min": "3.25rem",
            "max": "4.0rem"
          },
          "size": "3.75rem",
          "slug": "gigunda"
        }
      ]
    },
    "layout": {
      "contentSize": "700px",
      "wideSize": "1100px"
    },
    "appearanceTools": true,
    "spacing": {
      "margin": true,
      "padding": true,
      "units": ["%", "px", "em", "rem", "vh", "vw"],
      "blockGap": true
    },
    "border": {
      "color": true
    },
    "color": {
      "link": true,
      "custom": true,
      "customGradient": true,
      "customDuotone": true,

      "palette": [
        {
          "slug": "background-color",
          "color": "#ffffff",
          "name": "Background"
        },
        {
          "slug": "contrast",
          "color": "#000000",
          "name": "Contrast"
        },
        {
          "slug": "foreground-color",
          "color": "#333333",
          "name": "Foreground"
        },
        {
          "slug": "primary",
          "color": "#19a04c",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#ffffff",
          "name": "Secondary"
        },
        {
          "slug": "tertiary",
          "color": "#eeeeee",
          "name": "Tertiary"
        },
        {
          "slug": "accent",
          "color": "#ffcc66",
          "name": "Accent"
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--poppins)",
      "fontSize": "var(--wp-preset--font-size--small)"
    },
    "color": {
      "background": "var(--wp--preset--color--background-color)",
      "text": "var(--wp--preset--color--foreground-color)"
    },
    "blocks": {
      "core/post-content": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        },
        "color": {
          "background": "var(--wp--preset--color--background-color)",
          "text": "var(--wp--preset--color--foreground-color)"
        }
      },
      "core/navigation": {
        "color": {
          "background": "none",
          "text": "var(--wp--preset--color--primary)"
        },
        "elements": {
          "link": {
            "typography": {
              "textDecoration": "none",
              "fontFamily": "var(--wp--preset--font-family--poppins)"
            },
            "border": {
              "bottom": {
                "width": "0"
              }
            },
            ":hover": {
              "typography": {
                "textDecoration": "none"
              },
              "border": {
                "bottom": { "width": "0" }
              }
            },
            ":active": {
              "color": {
                "text": "#ff0000 !i"
              }
            }
          }
        }
      }
    },

    "elements": {
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "textTransform": "uppercase"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "lineHeight": "1.2"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",

          "lineHeight": "1.2"
        }
      },
      "button": {
        "border": {
          "radius": "0"
        },
        "color": {
          "text": "var(--wp--preset--color--contrast)",
          "background": "var(--wp--preset--color--accent)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "textDecoration": "none",
          "fontWeight": "bold"
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--primary)",
            "text": "var(--wp--preset--color--secondary) !important"
          }
        }
      },

      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "textDecoration": "none"
        },
        "border": {
          "bottom": {
            "width": "2px",
            "color": "transparent",
            "style": "solid"
          }
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--accent)"
          },
          "typography": {
            "textDecoration": "none"
          },
          "border": {
            "bottom": {
              "width": "2px",
              "color": "var(--wp--preset--color--primary)",
              "style": "solid"
            }
          }
        }
      }
    }
  }
}
